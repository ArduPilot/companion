      <visual name='visual'>
        <pose>0 -0.1 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.02 0.25 0.02</size>
            </box>
          </geometry>
      </visual>
      <sensor type="multicamera" name="stereo_camera">
            <update_rate>10.0</update_rate>
            <camera name="left">
              <horizontal_fov>1.3962634</horizontal_fov>
              <image>
                <width>640</width>
                <height>480</height>
                <format>R8G8B8</format>
              </image>
              <clip>
                <near>0.02</near>
                <far>300</far>
              </clip>
              <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.007</stddev>
              </noise>
            </camera>
            <camera name="right">
              <pose>0 -0.2 0 0 0 0</pose>
              <horizontal_fov>1.3962634</horizontal_fov>
              <image>
                <width>640</width>
                <height>480</height>
                <format>R8G8B8</format>
              </image>
              <clip>
                <near>0.02</near>
                <far>300</far>
              </clip>
              <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.007</stddev>
              </noise>
            </camera>
          </sensor>
  </link>
