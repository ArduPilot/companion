    <link name="cam_link">
      <pose>0.0 0.1 0.1 0 0 0 </pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <iyy>0.0001</iyy>
          <izz>0.0001</izz>
        </inertia>
